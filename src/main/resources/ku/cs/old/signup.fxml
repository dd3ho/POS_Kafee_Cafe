<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>


<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ku.cs.controllers.old.SignupController">
   <children>
      <VBox fx:id="vbox" layoutX="78.0" layoutY="42.0" prefHeight="515.0" prefWidth="625.0">
          <Label text="Sign Up" />
          <TextField fx:id="firstNameField" promptText="First Name" />
          <TextField fx:id="lastNameField" promptText="Last Name" />
          <TextField fx:id="phoneNumberField" promptText="Phone Number" />
          <Button onAction="#takePhoto" text="Take Photo" />
          <Button onAction="#signup" text="Sign Up" />
      </VBox>
   </children>
</AnchorPane>
