<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>

<BorderPane fx:id="borderPane" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/21" fx:controller="ku.cs.controllers.FaceRecognitionController">
    <center>
        <VBox alignment="CENTER" spacing="10">
            <Label text="Face Recognition" />
            <Button onAction="#recognizeFace" text="Recognize Face" />
         <Label prefHeight="60.0" prefWidth="81.0" text="Label" />
            <ImageView fx:id="imageView" fitHeight="200" fitWidth="200" preserveRatio="true" />
        </VBox>
    </center>
</BorderPane>
